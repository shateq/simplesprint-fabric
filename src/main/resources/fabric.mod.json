{
  "schemaVersion": 1,
  "id": "simplesprint",
  "version": "${version}",
  "license": "MIT",
  "icon": "assets/icon.png",
  "name": "SimpleSprint Fabric",
  "description": "Why would you spam this sprint key bind?",
  "authors": [
    "Shateq", "LambdAurora - SpruceUI creator - Inspiration to test it out", "OrangeMarshall - Formal Author"
  ],
  "contact": {
    "homepage": "https://github.com/shateq/simplesprint-fabric",
    "sources": "https://github.com/shateq/simplesprint-fabric.git",
    "issues": "https://github.com/shateq/simplesprint-fabric/issues"
  },
  
  "environment": "client",
  "entrypoints": {
    "client": [
      "shateq.mods.fabric.simplesprint.SimpleSprint"
